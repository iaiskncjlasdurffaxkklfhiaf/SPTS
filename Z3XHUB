	token.TextScaled = true
	token.TextSize = 14.000
	token.TextWrapped = true
	token.TextXAlignment = Enum.TextXAlignment.Left
	
	local FSV1 = game.Players.LocalPlayer.PrivateStats.FistStrength.Value
	local BTV1 = game.Players.LocalPlayer.PrivateStats.BodyToughness.Value
	local PPV1 = game.Players.LocalPlayer.PrivateStats.PsychicPower.Value
	local SPEEDV1 = game.Players.LocalPlayer.PrivateStats.MovementSpeed.Value
	local JUMPV1 = game.Players.LocalPlayer.PrivateStats.JumpForce.Value
	local Token1 = game.Players.LocalPlayer.PrivateStats.Token.Value
	
	local function MSUSL_fake_script()
		_G.as = true
		while true do
			if _G.as == true then
				task.wait(0)
				local fs2 = game.Players.LocalPlayer.PrivateStats.FistStrength.Value
				local bt2 = game.Players.LocalPlayer.PrivateStats.BodyToughness.Value
				local pp2 = game.Players.LocalPlayer.PrivateStats.PsychicPower.Value
				local speed2 = game.Players.LocalPlayer.PrivateStats.MovementSpeed.Value
				local jump2 = game.Players.LocalPlayer.PrivateStats.JumpForce.Value
				local Token2 = game.Players.LocalPlayer.PrivateStats.Token.Value
				fs.Text = "Fist : " .. converttoletter(fs2 - FSV1)
				bt.Text = "Body : " .. converttoletter(bt2 - BTV1)
				pp.Text = "Psychic : " .. converttoletter(pp2 - PPV1)
				speed.Text = "Speed : " .. converttoletter(speed2 - SPEEDV1)
				jump.Text = "Jump : " .. converttoletter(jump2 - JUMPV1)
				token.Text = "Token : " .. converttoletter(Token2 - Token1)
			end
		end
	end
	coroutine.wrap(MSUSL_fake_script)()	
end
})
Tabs.Farm:AddButton({
	Title = "GodMode",
	Description = "",
	Callback = function()
		Fluent:Notify({
			Title = "Godmode",
			Content = 'Press V to move',
			SubContent = "",
			Duration = 5
		})
		game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.Text = "GodMode"
		game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
		--game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
		game:GetService("ReplicatedStorage").GhostRiderPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestMsgFrame:GetPropertyChangedSignal("Visible"):Connect(function()
			game:GetService("ReplicatedStorage").GhostRiderPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			task.wait(1)
			--game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
			stopQuest = true;
			--[[function NoKill()
				local R = game:GetService 'Players'.LocalPlayer.Character;
				local S = getrawmetatable(game)
				local T = S.__index;
				local U = S.__namecall;
				setreadonly(S, false)
				S.__namecall = newcclosure(function(V, ...)
					local W = {...}
					local X = getnamecallmethod()
					if not checkcaller() and V == R and X == "BreakJoints" then
						return nil
					end
					return U(V, ...)
				end)
				setreadonly(S, true)
			end]]
			function fixCam()
				local Y = workspace.CurrentCamera;
				Y.CameraSubject = game.Players['LocalPlayer'].Character.Humanoid;
				Y.CameraType = Enum.CameraType.Custom
			end
			--[[function SetSpeed(Z)
				local _ = assert(tonumber(Z), 'Must be a number')
				game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(Z)
			end]]
			--[[function CanAttack()
				local a0 = game:GetService("Players").LocalPlayer.PlayerScripts.MainScript;
				while stopQuest do
					wait(0.01)
					getsenv(a0).TalkingOnQuest = false
				end
			end]]
			--NoKill()
			fixCam()
			--CanAttack()
			return
			game.Players.LocalPlayer:GetMouse().KeyDown:connect(function(key)
				if key == "v" then
					if game.Players.LocalPlayer:GetMouse().Target then
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer:GetMouse().Hit.x, game.Players.LocalPlayer:GetMouse().Hit.y + 5, game.Players.LocalPlayer:GetMouse().Hit.z)
					end
				end
			end)
		end)
		repeat wait() until tostring(game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.AbsolutePosition) == '743.039978, 52.1999969'
		firesignal(game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.MouseButton1Click)
		task.wait(3)
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"QuestTalkStart"})
		game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.QuestMsgFrame.Visible = true
end
})
