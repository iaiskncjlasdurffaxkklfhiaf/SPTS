--// functions toggls
Toggls.fs:OnChanged(function()
	while Fluent.Options.F1.Value and task.wait(0) do
		pcall(function()
			if getfspos() ~= null then
				workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame = CFrame.new(getfspos())
			end
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ getfs() })
		end)
	end
end)

Toggls.bt:OnChanged(function()
	while Fluent.Options.F2.Value and task.wait(0) do
		pcall(function()
			if getbtpos ~= null then
				workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame = CFrame.new(getbtpos())
			else
				game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "+BT1" })
			end
		end)
	end
end)

Toggls.pp:OnChanged(function()
	while Fluent.Options.F3.Value and task.wait(0) do
		pcall(function()
			if getpppos() ~= null then
				workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame = CFrame.new(getpppos())
			end
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ getpp() })
		end)
	end
end)

Toggls.js:OnChanged(function()
	while Fluent.Options.F4.Value and task.wait(0) do
		pcall(function()
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"Weight", "Weight4"})
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"+MS5"})
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"+JF5"})
		end)
	end
end)

local RespBox = Tabs.Farm:AddSlider("Slider", {Title = "Respawn",Description = "",Default = 50,Min = 1,Max = 99,Rounding = 1,
	Callback = function(Value)
		settings['resp_hp'] = Value / 100
	end
})

spawn(function()
    while task.wait() do
        pcall(function()
            game:GetService("Players").LocalPlayer.PlayerGui.IntroGui.Enabled = false
            game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.Enabled = true
            game:GetService("Lighting").Blur.Enabled = false
        end)
    end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
	character:WaitForChild('Humanoid'):GetPropertyChangedSignal('Health'):Connect(function()
		if character:WaitForChild('Humanoid').MaxHealth*(settings['resp_hp']) > character:WaitForChild('Humanoid').Health then
			game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
			pos = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
			character:Destroy()
			repeat task.wait(0) until workspace:FindFirstChild(game.Players.LocalPlayer.Name)
			task.wait(0.6)
			game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(pos)
		end
	end)
end)

Toggls.blaser:OnChanged(function()
	while Fluent.Options.U3.Value and task.wait(0) do
		pcall(function()
			for _, v in pairs(workspace:GetChildren()) do
				if v.Name == "Part" then
					v.Transparency = 0
					v.Color = Color3.fromRGB(0, 0, 0)
				end
			end
		end)
	end
end)

Toggls.rlaser:OnChanged(function()
	while Fluent.Options.U4.Value and task.wait(0) do
		pcall(function()
			for _, v in pairs(workspace:GetChildren()) do
				if v.Name == "Part" then
					v.Transparency = 0
					v.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
				end
			end
		end)
	end
end)

Toggls.esp:OnChanged(function()
	if Fluent.Options.U5.Value then
		for _, v in next, Plrs:GetPlayers() do
			spawn(function()
				if v ~= MyPlr then
					if workspace:FindFirstChild(v.Name) then
						CreateESP(workspace[v.Name])
					end
				end
			end)
		end
	else
		workspace.ESPFolder:ClearAllChildren()
	end
end)

Toggls.daynight:OnChanged(function()
	while Fluent.Options.V1.Value and task.wait(0) do
		if Fluent.Options.V1.Value then
			pcall(function()
				game:GetService("Lighting").TimeOfDay = "24:00:00"
				game:GetService("Lighting").ClockTime = 24
				game:GetService("Lighting").Brightness = 1
			end)
		elseif not Fluent.Options.V1.Value then
			pcall(function()
				game:GetService("Lighting").TimeOfDay = "12:00:00"
				game:GetService("Lighting").ClockTime = 12
				game:GetService("Lighting").Brightness = 1
			end)
		end
	end
end)

Toggls.autoc:OnChanged(function()
	while Fluent.Options.P1.Value and task.wait(0) do
		pcall(function()
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "Skill_Punch", "Left" })
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "Skill_Punch", "Right" })
		end)
	end
end)

Toggls.spamr:OnChanged(function()
	while Fluent.Options.P2.Value and task.wait(0) do
		coroutine.wrap(function()
			pcall(function()
				game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "Skill_SpherePunch", Vector3.new(x, 1e20, z) })
				for i, v in pairs(workspace.Storage:GetChildren()) do
					if v.Name == getball() then
						v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					end
				end
			end)
		end)()
	end
end)

Toggls.soulat:OnChanged(function()
	while Fluent.Options.P3.Value and task.wait(0) do
		pcall(function()
			if targets[1] then
				for i, v in next, targets do
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[targets[i]].Character
					.HumanoidRootPart.CFrame + game.Players[targets[i]].Character.HumanoidRootPart.CFrame.upVector * 40
					game.ReplicatedStorage.RemoteEvent:FireServer({ "Skill_SoulAttack_Start", game.Players[targets[i]] })
					game.ReplicatedStorage.RemoteEvent:FireServer({ "Skill_SoulAttack_End" })
				end
			end
		end)
	end
end)

Toggls.soulr:OnChanged(function()
	while Fluent.Options.P4.Value and task.wait(0) do
		pcall(function()
			if targets[1] then
				for i, v in next, targets do
					if workspace:FindFirstChild(targets[i]) then
						if workspace[targets[i]].Humanoid.Health > 0 and not workspace[targets[i]]:FindFirstChild('ForceField') and not workspace[targets[i]]:FindFirstChild('SafeZoneShield') then
							game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "Skill_SoulReap", game.Players[targets[i]] })
							repeat task.wait(0) until workspace[targets[i]].Humanoid.Health == 0
						end
					end
				end
			end
		end)
	end
end)

coroutine.wrap(function()
	while task.wait(0) do
		pcall(function()
			if Fluent.Options.P5.Value then
				if targets[1] then
					for i, v in next, targets do
						if workspace:FindFirstChild(targets[i]) then
							if not workspace[targets[i]]:FindFirstChild('ForceField') and not workspace[targets[i]]:FindFirstChild('SafeZoneShield') and workspace[targets[i]].Humanoid.Health ~= 0 then
								workspace[targets[i]].HumanoidRootPart.CFrame = workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame + workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.CFrame.LookVector * 1
							end
						end
					end
				end
			end
		end)
	end
end)()

Toggls.cbringall:OnChanged(function()
	while Fluent.Options.P6.Value and task.wait(0) do
		pcall(function()
			for _, player in pairs(game.Players:GetPlayers()) do
				player.Character:MoveTo(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
			end
		end)
	end
end)

Toggls.sitfun:OnChanged(function()
	while Fluent.Options.P7.Value and task.wait(0) do
		pcall(function()
			if targets[1] then
				for i, v in next, targets do
					game.Players.LocalPlayer.Character.Humanoid.Sit = true
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
					workspace[targets[i]].Head.CFrame * CFrame.new(0,2.5, 0)
				end
			end
		end)
	end
end)

Toggls.spamtp:OnChanged(function()
	while Fluent.Options.P8.Value and task.wait(0) do
		pcall(function()
			if targets[1] then
				for i, v in next, targets do
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
					game.Players[targets[i]].Character.HumanoidRootPart.CFrame
				end
			end
		end)
	end
end)

Toggls.stackall:OnChanged(function()
	while Fluent.Options.P9.Value and task.wait(0) do
		pcall(function()
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer({ "Skill_SpherePunch", Vector3.new(0, 9e20, 0) })
			for i,v in pairs(game.Players:GetPlayers()) do
				coroutine.wrap(function()
					pcall(function()
						if v ~= game.Players.LocalPlayer then
							if v.PrivateStats.BodyToughness.Value <= game:GetService("Players").LocalPlayer.PrivateStats.FistStrength.Value * 2 then
								if v.Character.Humanoid.Health ~= 0 then
									if not v.Character:FindFirstChild("SafeZoneShield") and not v.Character:FindFirstChild('ForceField') then
										v.Character.HumanoidRootPart.CFrame = CFrame.new(0, 1e5, 0)
										for i, v in ipairs(game:GetService("Workspace").Storage:GetChildren()) do
											if v.Name == "EnergySphere9" then
												v.CFrame = CFrame.new(0, 1e5, 0)
											end
										end
									end
								end
							end
						end
					end)
				end)()
			end
		end)
	end
end)

Toggls.soulrall:OnChanged(function()
	while Fluent.Options.P10.Value do
		task.wait(0)
		pcall(function()
			for i, v in pairs(game.Players:GetPlayers()) do
				local Random = game.Players:GetPlayers()[math.random(1, #game.Players:GetPlayers())]
				if Random.PrivateStats.PsychicPower.Value <= game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value * 1000 then
					for i, v in pairs(game.Players:GetPlayers()) do
						game.ReplicatedStorage.RemoteEvent:FireServer({ "Skill_SoulReap", game.Players[v.Name] })
					end
				end
			end
		end)
	end
end)

Toggls.ppall:OnChanged(function()
	while Fluent.Options.P11.Value do
		task.wait(0)
		pcall(function()
			for i, v in pairs(game.Players:GetPlayers()) do
				local Random = game.Players:GetPlayers()[math.random(1, #game.Players:GetPlayers())]
				if Random.PrivateStats.PsychicPower.Value <= game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value * 100 then
					game.ReplicatedStorage.RemoteEvent:FireServer({ "Skill_SoulAttack_Start", game.Players[v.Name] })
					game.ReplicatedStorage.RemoteEvent:FireServer({ "Skill_SoulAttack_End" })
				end
			end
		end)
	end
end)

