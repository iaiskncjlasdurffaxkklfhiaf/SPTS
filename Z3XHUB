Tabs.Farm:AddButton({
	Title = "StatCounter",
	Description = "",
	Callback = function()
	for i, v in next, game.Players.LocalPlayer.PlayerGui.Fluent:GetChildren() do
		if v.Name == "statplus" then
			v:Destroy()
		end
	end
	local statplus = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local Frame = Instance.new("Frame")
	local fs = Instance.new("TextLabel")
	local UICorner = Instance.new("UICorner")
	local bt = Instance.new("TextLabel")
	local pp = Instance.new("TextLabel")
	local speed = Instance.new("TextLabel")
	local jump = Instance.new("TextLabel")
	local token = Instance.new("TextLabel")
	
	statplus.Name = "statplus"
	statplus.Parent = game.Players.LocalPlayer.PlayerGui.Fluent
	statplus.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	statplus.ResetOnSpawn = false
	
	Frame.Parent = statplus
	Frame.BackgroundColor3 = Color3.fromRGB(0,0,0)
	Frame.BackgroundTransparency = 0
	Frame.Position = UDim2.new(0, 0, 0.4, 0)
	Frame.Size = UDim2.new(0, 150, 0, 200)
	
	fs.Name = "fs"
