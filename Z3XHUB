repeat task.wait(0) until game:IsLoaded()

spawn(function()
	if workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
		game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
	end
end)

for i, v in pairs(game:GetService('Players'):GetChildren()) do
	if v ~= game.Players.LocalPlayer then
		for i2, v2 in pairs(v:GetChildren()) do
			if v2.Name == 'LocalSoundFolder' then
				v2:Destroy()
			end
		end
	end
end

game:GetService('Players').PlayerAdded:Connect(function()
	wait()
	for i, v in pairs(game:GetService('Players'):GetChildren()) do
		if v ~= game.Players.LocalPlayer then
			for i2, v2 in pairs(v:GetChildren()) do
				if v2.Name == 'LocalSoundFolder' then
					v2:Destroy()
				end
			end
		end
	end
end)
game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("BubbleChat").DescendantAdded:Connect(function(Descendant)
	if Descendant:IsA("ImageLabel") then
		Descendant.ImageColor3 = Color3.fromRGB(59, 59, 59)
	elseif Descendant:IsA("TextLabel") then
		Descendant.TextColor3 = Color3.new(1,1,1)
	end
end)

for i,v in next, workspace:GetChildren() do
	if v.Name == "SoulAttackBeam1" or v.Name == "SoulAttackBeam2" then
		v:Destroy()
	end
end

for i,v in next, workspace:GetChildren() do
	if v.Name == "ESPFolder" then
		v:Destroy()
	end
end

for i,v in next, game.Players.LocalPlayer.PlayerGui.ScreenGui:GetChildren() do
	if v.Name == "XpFrame" then
		v:Destroy()
	end
end

workspace.ChildAdded:Connect(function(child)
	task.wait(1)
	if child.Name == "SoulAttackBeam1" or child.Name == "SoulAttackBeam2" then
		child:Destroy()
	end
end)

for i,v in next, game.Players.LocalPlayer.PlayerGui:GetChildren() do
	if v.Name == "Fluent" then
		v:Destroy()
	end
end

local Fluent = loadstring(game:HttpGet('https://raw.githubusercontent.com/Wer4er/SPTS/main/Fluent'))()
local InterfaceManager = loadstring(game:HttpGet('https://raw.githubusercontent.com/Wer4er/SPTS/main/FluentInterfaceManager'))()

settings = {
	resp_hp = 0.5,
}

local Plrs = game:GetService("Players")
local MyPlr = Plrs.LocalPlayer
local MyChar = MyPlr.Character

function round(num, numDecimalPlaces)
	local mult = 10 ^ (numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end

function converttoletter(num)
	if num / 1e21 >= 1 then
		newnum = num / 1e21
		return round(newnum, 2) .. "Sx"
	elseif num / 1e18 >= 1 then
		newnum = num / 1e18
		return round(newnum, 2) .. "Qi"
	elseif num / 1e15 >= 1 then
		newnum = num / 1e15
		return round(newnum, 2) .. "Qa"
	elseif num / 1e12 >= 1 then
		newnum = num / 1e12
		return round(newnum, 2) .. "T"
	elseif num / 1e09 >= 1 then
		newnum = num / 1e09
		return round(newnum, 2) .. "B"
	elseif num / 1e06 >= 1 then
		newnum = num / 1e06
		return round(newnum, 2) .. "M"
	elseif num / 1e03 >= 1 then
		newnum = num / 1e03
		return round(newnum, 2) .. "K"
	else
		return num
	end
end

function updateColor(status)
	if status == "Criminal" then
		return Color3.new(1, 0.1, 1)
	elseif status == "Lawbreaker" then
		return Color3.new(1, 0.1, 0.1)
	elseif status == "Guardian" then
		return Color3.new(0.1, 0.8, 1)
	elseif status == "Protector" then
		return Color3.new(0.1, 0.1, 1)
	elseif status == "Supervillain" then
		return Color3.new(0.3, 0.1, 0.1)
	elseif status == "Superhero" then
		return Color3.new(0.8, 0.8, 0)
	else
		return Color3.new(1, 1, 1)
	end
end

if not workspace:FindFirstChild('ESPFolder') then
	local espfolder = Instance.new('Folder', workspace)
	espfolder.Name = "ESPFolder"
	espfolder.ChildAdded:Connect(function(child)
		if not Fluent.Options.U5.Value then
			task.wait()
			child:Destroy()
		end
	end)
end

function CreateESP(character)
	coroutine.wrap(function()
		local plr = game.Players[character.Name]

		repeat task.wait() until character:FindFirstChild("Head")
		local head = character.Head

		local bb = Instance.new("BillboardGui", workspace.ESPFolder)
		bb.Adornee = head
		bb.ExtentsOffset = Vector3.new(0, 1, 0)
		bb.AlwaysOnTop = true
		bb.Size = UDim2.new(0, 2, 0, 2)
		bb.StudsOffset = Vector3.new(0, 3, 0)
		bb.Name = plr.Name

		local frame = Instance.new("Frame", bb)
		frame.ZIndex = 10
		frame.BackgroundTransparency = 1
		frame.Size = UDim2.new(5, 0, 5, 0)

		local TxtName = Instance.new("TextLabel", frame)
		TxtName.Name = "Names"
		TxtName.ZIndex = 10
		TxtName.Text = plr.Name
		TxtName.BackgroundTransparency = 1
		TxtName.Position = UDim2.new(0, 0, 0, -55)
		TxtName.Size = UDim2.new(1, 0, 10, 0)
		TxtName.Font = "SourceSansBold"
		TxtName.TextColor3 = updateColor(plr.leaderstats.Status.Value)
		TxtName.TextSize = 15
		TxtName.TextStrokeTransparency = 0.5

		local TxtDist = Instance.new("TextLabel", frame)
		TxtDist.Name = "Dist"
		TxtDist.ZIndex = 10
		TxtDist.Text = ""
		TxtDist.BackgroundTransparency = 1
		TxtDist.Position = UDim2.new(0, 0, 0, -45)
		TxtDist.Size = UDim2.new(1, 0, 10, 0)
		TxtDist.Font = "SourceSansBold"
		TxtDist.TextColor3 = Color3.new(1, 1, 1)
		TxtDist.TextSize = 15
		TxtDist.TextStrokeTransparency = 0.5

		local TxtHealth = Instance.new("TextLabel", frame)
		TxtHealth.Name = "Health"
		TxtHealth.ZIndex = 10
		TxtHealth.Text = (character:FindFirstChild("Humanoid")) and ("Health: " .. converttoletter(character.Humanoid.Health) .. "/" .. converttoletter(character.Humanoid.MaxHealth)) or "Error"
		TxtHealth.BackgroundTransparency = 1
		TxtHealth.Position = UDim2.new(0, 0, 0, -35)
		TxtHealth.Size = UDim2.new(1, 0, 10, 0)
		TxtHealth.Font = "SourceSansBold"
		TxtHealth.TextColor3 = Color3.new(1, 1, 1)
		TxtHealth.TextSize = 15
		TxtHealth.TextStrokeTransparency = 0.5

		local TxtBody = Instance.new("TextLabel", frame)
		TxtBody.Name = "Body"
		TxtBody.ZIndex = 10
		TxtBody.Text = "Body: " .. converttoletter(plr.PrivateStats.BodyToughness.Value)
		TxtBody.BackgroundTransparency = 1
		TxtBody.Position = UDim2.new(0, 0, 0, -25)
		TxtBody.Size = UDim2.new(1, 0, 10, 0)
		TxtBody.Font = "SourceSansBold"
		TxtBody.TextColor3 = Color3.new(1, 1, 1)
		TxtBody.TextSize = 15
		TxtBody.TextStrokeTransparency = 0.5

		local TxtFist = Instance.new("TextLabel", frame)
		TxtFist.Name = "Fist"
		TxtFist.ZIndex = 10
		TxtFist.Text = "Fist: " .. converttoletter(plr.PrivateStats.FistStrength.Value)
		TxtFist.BackgroundTransparency = 1
		TxtFist.Position = UDim2.new(0, 0, 0, -15)
		TxtFist.Size = UDim2.new(1, 0, 10, 0)
		TxtFist.Font = "SourceSansBold"
		TxtFist.TextColor3 = Color3.new(1, 1, 1)
		TxtFist.TextSize = 15
		TxtFist.TextStrokeTransparency = 0.5

		local TxtPsychic = Instance.new("TextLabel", frame)
		TxtPsychic.Name = "Psychic"
		TxtPsychic.ZIndex = 10
		TxtPsychic.Text = "Psychic: " .. converttoletter(plr.PrivateStats.PsychicPower.Value)
		TxtPsychic.BackgroundTransparency = 1
		TxtPsychic.Position = UDim2.new(0, 0, 0, -5)
		TxtPsychic.Size = UDim2.new(1, 0, 10, 0)
		TxtPsychic.Font = "SourceSansBold"
		TxtPsychic.TextColor3 = Color3.new(1, 1, 1)
		TxtPsychic.TextSize = 15
		TxtPsychic.TextStrokeTransparency = 0.5

		pcall(function()
			coroutine.wrap(function()
				while task.wait() do
					pcall(function()
						TxtDist.Text = "Distance: " .. math.floor((workspace[game.Players.LocalPlayer.Name].HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude)
					end)
				end
			end)()
			
			if not character:FindFirstChild('Humanoid') then
				repeat task.wait() until character:FindFirstChild('Humanoid')
			end
			character.Humanoid:GetPropertyChangedSignal('Health'):Connect(function()
				TxtHealth.Text = "Health: " ..
				converttoletter(character.Humanoid.Health) .. "/" .. converttoletter(character.Humanoid.MaxHealth)
				if character.Humanoid.Health <= 0 then
					bb:Destroy()
				end
			end)
		end)
	end)()
end

local playerlist = {};
for i, v in pairs(game.Players:GetPlayers()) do
	table.insert(playerlist, v.Name)
end

function getfs()
	if game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e13 then
		return "+FS6"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e11 then
		return "+FS5"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e9 then
		return "+FS4"
	else
		return "+FS3"
	end
end



function getpp()
	if game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e15 then
		return "+PP6"
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e12 then
		return "+PP5"
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e9 then
		return "+PP4"
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e6 then
		return "+PP3"
	else
		return "+PP2"
	end
end

function getfspos()
	if game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e13 then
		return -364.097198, 15735.1455, -7.32395744
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e11 then
		return 1380.3583984375, 9273.736328125, 1652.6295166015625
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value >= 1e9 then
		return 1175.2962646484375, 4789.25341796875, -2293.052978515625
	end
end

function getbtpos()
	if game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e13 then
		return -278.3638916015625, 281.3877868652344, 1005.1320190429688
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e11 then
		return -275.5314025878906, 281.6444396972656, 991.4784545898438
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e9 then
		return -244.4465789794922, 287.1148986816406, 978.9732666015625
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e7 then
		return -2024.790283203125, 714.248779296875, -1882.2078857421875
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e6 then
		return -2297.46484375, 976.7352294921875, 1075.0213623046875
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e5 then
		return 1634.4921875, 259.62298583984375, 2248.2060546875
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e4 then
		return 357.2840881347656, 263.7413635253906, -492.33245849609375
	elseif game.Players.LocalPlayer.PrivateStats.BodyToughness.Value >= 4e2 then
		return 367.770751953125, 249.7052459716797, -444.747802734375
	end
end

function getpppos()
	if game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e15 then
		return -2545.082763671875, 5412.32958984375, -491.8777160644531
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e12 then
		return -2581.781005859375, 5516.38818359375, -500.4078674316406
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e9 then
		return -2561.86328125, 5500.8828125, -440.46112060546875
	elseif game.Players.LocalPlayer.PrivateStats.PsychicPower.Value >= 1e6 then
		return -2529.778076171875, 5486.39208984375, -534.6697998046875
	end
end

function getball()
	if game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e15 then
		return "EnergySphere9"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e12 then
		return "EnergySphere8"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e10 then
		return "EnergySphere7"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e8 then
		return "EnergySphere6"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e7 then
		return "EnergySphere5"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e6 then
		return "EnergySphere4"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e5 then
		return "EnergySphere3"
	elseif game.Players.LocalPlayer.PrivateStats.FistStrength.Value > 1e4 then
		return "EnergySphere2"
	else
		return "EnergySphere1"
	end
end

local Window = Fluent:CreateWindow({
	Title = "Z3XHub 2.3",
	SubTitle = "by Wer4eryyy",
	TabWidth = 160,
	Size = UDim2.fromOffset(580, 460),
	Position = UDim2.fromOffset(0, 0),
	Acrylic = false,
	Theme = "Darker",
	MinimizeKey = Enum.KeyCode.H
})

local Tabs = {
	Farm = Window:AddTab({ Title = "Farm & More", Icon = "axe" }),
	Visual = Window:AddTab({ Title = "Visual", Icon = "plus-circle" }),
	Usual = Window:AddTab({ Title = "Usual", Icon = "star" }),
	Fun = Window:AddTab({ Title = "Fun", Icon = "smile-plus" }),
	Teleports = Window:AddTab({ Title = "Teleports", Icon = "navigation-2" }),
	Pvp = Window:AddTab({ Title = "Pvp", Icon = "swords" }),
	Settings = Window:AddTab({ Title = "Settings", Icon = "settings" }),
};

Window:SelectTab(1)

local targets = {}
local target = game.Players.LocalPlayer.Name
local Input = Tabs.Pvp:AddInput("Input", {
	Title = "Targets",
	Default = "",
	Placeholder = "Nickname",
	Numeric = false,
	Finished = true,
	Callback = function(Value)
		for i, v in next, game.Players:GetChildren() do
			if string.find(string.lower(v.Name), string.lower(Value)) then
				target = v.Name
				break
			end
		end
		Fluent:Notify({
			Title = "Selected",
			Content = target,
			SubContent = "",
			Duration = 5
		})
	end
})

Tabs.Pvp:AddButton({
	Title = "Add",
	Description = "",
	Callback = function()
		if game.Players:FindFirstChild(target) then
			if not table.find(targets, target) then
				table.insert(targets, target)
			end
		end
		Fluent:Notify({
			Title = "Targets",
			Content = table.concat(targets, ', '),
			SubContent = "",
			Duration = 5
		})
	end
})
Tabs.Pvp:AddButton({
	Title = "Remove",
	Description = "",
	Callback = function()
		if table.find(targets, target) then
			for i, v in next, targets do
				if targets[i] == target then
					table.remove(targets, i)
				end
			end
		end
		Fluent:Notify({
			Title = "Targets",
			Content = table.concat(targets, ', '),
			SubContent = "",
			Duration = 5
		})
	end
})

