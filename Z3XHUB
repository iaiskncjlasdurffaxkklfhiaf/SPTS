if table.find({'Wer4er_official', 'Wer4erAlt', 'GaladonT'}, game.Players.LocalPlayer.Name) then
	setfpscap(60)
	Tabs.Settings:AddButton({
	Title = "RespDebug",
	Description = "",
	Callback = function()
		local connection
		s = tick()
		game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
		connection = game.Players.LocalPlayer.CharacterAdded:Connect(function()
			print("RESPAWN TIME: " .. tick() - s)
		end)
		local s;
		game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
			character.ChildAdded:Connect(function(child)
				if child.Name == "ForceField" then
				s = tick()
				end
			end)
			character.ChildRemoved:Connect(function(object)
				if object.Name == "ForceField" then
					print("FORCEFIELD TIME: " .. tick() - s)
				end
			end)
		end)
	end
	})
end

