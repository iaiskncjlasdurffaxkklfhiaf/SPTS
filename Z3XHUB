Tabs.Visual:AddButton({
	Title = "BlackMap",
	Description = "",
	Callback = function()
		local cry = workspace.Main.TrainingCrystal
		local mai = workspace.Main
		local town = workspace.Town
		workspace.Terrain.WaterColor = Color3.new(0,0,0)
		workspace.Terrain.WaterTransparency = 0
		workspace.Main.TrainingCrystal.Part.Color = Color3.new(0,0,0)
		for i,v in pairs(town.Color.Tree1.Leaf:GetChildren()) do task.wait()
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Tree2.Leaf:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Tree3.Leaf:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Tree1.Trunk:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Tree2.Trunk:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Tree3.Trunk:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(town.Color.Floor:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(mai.Volcano.Parts:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(cry.cloud:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(cry.CyanGem:GetChildren()) do
			if v:IsA('BasePart') then
				v.BrickColor = BrickColor.new(0,0,0)
			end
		end
		for i,v in pairs(mai.IceMountain:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(mai.Tornado:GetChildren()) do
			v.BrickColor = BrickColor.new(0,0,0)
		end
		for i,v in pairs(mai.TrainingCrystal.CyanGem:GetChildren()) do
			if v:IsA('BasePart') then
				v.BrickColor = BrickColor.new(0,0,0)
			end
		end
		for i,v in next, workspace.Town:GetChildren() do
			if v.Name == "Halloween" then
				v:Destroy()
			end
		end 
		for i,v in next, workspace.Town.Core:GetChildren() do
			if v.Name == "Part" then
				v.Material = Enum.Material.Ground
			end
		end
		for i,v in next, workspace.Town.Core:GetChildren() do
			if v.Name == "Union" then
				v.Material = Enum.Material.Ground
			end
		end 
		for i,v in next, workspace.Main:GetChildren() do
			if v.Name == "GhostRiderCollisionPart" then
				v:Destroy()
			end
		end
		for i,v in next, workspace.Main:GetChildren() do
			if v.Name == "GrimCollisionPart" then
				v:Destroy()
			end
		end
		for i, v in pairs(workspace.Town.Core:GetDescendants()) do
			if v:IsA("SpotLight") then
				v.Color = Color3.fromRGB(98, 0, 255)
				v:GetPropertyChangedSignal("Color"):Connect(function() 
					v.Color = Color3.fromRGB(98, 0, 255) 
				end)
			end
		end
	end
})
