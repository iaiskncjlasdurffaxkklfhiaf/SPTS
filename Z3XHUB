	token.Name = "token"
	token.Parent = Frame
	token.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	token.BackgroundTransparency = 1.000
	token.Position = UDim2.new(0, 0, 0.75, 0)
	token.Size = UDim2.new(0, 120, 0, 40)
	token.Font = Enum.Font.SourceSansBold
	token.Text = "Token : "
